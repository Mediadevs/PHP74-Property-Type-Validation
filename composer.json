{
    "name": "mediadevs/strict-php-oop",
    "description": "Enforcing strictly typed PHP OOP",
    "type": "library",
    "license": "MIT",
    "keywords": ["Strictly Typed PHP OOP", "OOP", "STRICT"],
    "authors": [
        {
            "name": "Mike van Diepen",
            "email": "mike.vandiepen@hotmail.com"
        }
    ],
    "version": "1.0.0",
    "autoload": {
        "psr-4": {
            "Mediadevs\\StrictPHP\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mediadevs\\StrictPHP\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.4",
        "nikic/php-parser": "^4.3"
    },
    "require-dev": {
        "phpunit/phpunit": "8",
        "phpstan/phpstan": "^0.11.0",
        "symfony/var-dumper": "^4.3",
        "squizlabs/php_codesniffer": "3.*",
        "nunomaduro/phpinsights": "^1.11.1",
        "php-coveralls/php-coveralls": "^2.0",
    },
    "bin": ["bin/strict-php-oop"]
}